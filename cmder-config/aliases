e.=explorer .
gl=git log --oneline --all --graph --decorate  $*
ls=ls --show-control-chars -F --color $*
ll=ls --show-control-chars -alF --color $*
pwd=cd
clear=cls
history=cat %CMDER_ROOT%\config\.history
unalias=alias /d $1

ealiases=gvim %CMDER_ROOT%\config\aliases
caliases=copy D:\app\cmder\config\aliases E:\runitonce\github\homeofunix\cmder-config\aliases
wget=wget -c --no-check-certificate $*
github=pushd E:\runitonce\github
one=pushd c:\users\douho\onedrive
ll=ls --show-control-chars -alF --color $*

dhcp=netsh interface ip set address "Ethernet" source=dhcp
dns=netsh interface ip set dnsservers "Ethernet" static $* primary
dns2=netsh interface ip add dnsservers "Ethernet" $* index=2
setip=netsh interface ip set address "Ethernet" static $*
addip=netsh interface ip add address "Ethernet" $*

